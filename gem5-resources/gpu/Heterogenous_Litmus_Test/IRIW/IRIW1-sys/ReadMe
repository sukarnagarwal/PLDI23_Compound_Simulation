// Command to create the binary, from the source litmus test file

sudo docker run --rm -v ${PWD}:${PWD} -w ${PWD} -u $UID:$GID gcn-gpu2 make

// Gem5 command to run the IRIW-1 Relax Hetero litmus test
sudo docker run -u $UID:$GID --volume $(pwd):$(pwd) -w $(pwd) gcn-gpu2 gem5/build/GCN3_X86/gem5.opt --stats-file=stats_IRIW_1_Relax_Hetro_2048runs gem5/configs/example/apu_se.py --cpu-type=DerivO3CPU -n 16 -c gem5-resources/gpu/Heterogenous_Litmus_Test/IRIW/bin/IRIW_Ver1_Relax -o 8


