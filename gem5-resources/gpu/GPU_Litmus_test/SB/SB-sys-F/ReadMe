// Command to create the binary, from the source litmus test file

sudo docker run --rm -v ${PWD}:${PWD} -w ${PWD} -u $UID:$GID gcn-gpu2 make



// Gem5 command to run the SB Fence GPU litmus test


sudo docker run -u $UID:$GID --volume $(pwd):$(pwd) -w $(pwd) gcn-gpu2 gem5/build/GCN3_X86/gem5.opt --stats-file=stats_SB_Fence_GPU_only_2048runs gem5/configs/example/apu_se.py --cpu-type=DerivO3CPU -n 3 -c gem5-resources/gpu/GPU_Litmus_test/SB/SB-sys-F/bin/SB_RelAcq_WO_Atomic_Fence



